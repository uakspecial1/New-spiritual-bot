{
    "version": 2,
    "builds": [
      {
        "src": "main_and_final_chunking.py",
        "use": "@vercel/python"
      },
      {
        "src": "Pre_Processing_main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main_and_final_chunking.py"
      },
      {
        "src": "/preprocess",
        "dest": "Pre_Processing_main.py"
      }
    ]
  }
  